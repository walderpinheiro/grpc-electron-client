{
  "name": "@walderpinheiro/grpc-electron-client",
  "version": "1.0.0",
  "description": "A dependecy to using grpc in an SPA inside a electron application without a proxy",
  "main": "dist/index.js",
  "bin": {
    "grpc-electron-client": "dist/build.js"
  },
  "scripts": {
    "build": "uglifyjs --compress -m reserved=['$','require','exports'] -- index.js"
  },
  "keywords": [
