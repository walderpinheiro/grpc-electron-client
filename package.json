    "electron",
    "grpc web"
  ],
  "author": "Walder Pinheiro Leite Filho",
  "license": "ISC",
  "dependencies": {
    "@grpc/proto-loader": "^0.5.6",
    "grpc": "^1.24.5",
    "electron": "9.3.5"
  },
  "peerDependencies": {
    "electron": "9.3.5"
  },
  "devDependencies": {
    "uglify-js": "^3.12.8"
  }
}
